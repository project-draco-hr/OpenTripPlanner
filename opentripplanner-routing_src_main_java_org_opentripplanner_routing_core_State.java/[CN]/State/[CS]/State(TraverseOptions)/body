{
  this.time=opt.dateTime.getTime() / 1000;
  this.weight=0;
  this.vertex=opt.getOriginVertex();
  this.backState=null;
  this.backEdge=null;
  this.backEdgeNarrative=null;
  this.hops=0;
  this.stateData=new StateData();
  stateData.options=opt;
  stateData.startTime=time;
  stateData.tripSeqHash=0;
}
