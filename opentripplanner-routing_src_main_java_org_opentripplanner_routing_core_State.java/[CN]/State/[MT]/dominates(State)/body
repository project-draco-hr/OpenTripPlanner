{
  if (other.weight == 0) {
    return false;
  }
  if (isBikeRenting() != other.isBikeRenting())   return false;
  if (this.similarRouteSequence(other)) {
    return walkDistance <= other.getWalkDistance() * 1.05 && this.weight <= other.weight * 1.05;
  }
  double weightDiff=this.weight / other.weight;
  return walkDistance <= other.getWalkDistance() * 1.05 && (weightDiff < 1.05 && this.weight - other.weight < 30) && this.getElapsedTime() - other.getElapsedTime() <= 30;
}
