{
  if (isBikeRenting() != other.isBikeRenting()) {
    return false;
  }
  if (backEdge != other.getBackEdge() && ((backEdge instanceof PlainStreetEdge) && (!((PlainStreetEdge)backEdge).getTurnRestrictions().isEmpty())))   return false;
  return walkDistance <= other.getWalkDistance() * Raptor.WALK_EPSILON && this.getElapsedTimeSeconds() <= other.getElapsedTimeSeconds() + 30 && getNumBoardings() <= other.getNumBoardings();
}
