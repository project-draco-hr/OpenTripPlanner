{
  super(in.getToVertex(),out.getFromVertex());
  double angleDiff=Math.abs(DirectionUtils.getLastAngle(in.getGeometry()) - DirectionUtils.getFirstAngle(out.getGeometry()));
  turnAngle=(int)(180 * angleDiff / Math.PI);
  if (turnAngle > 180) {
    turnAngle=360 - turnAngle;
  }
  from=in.getName();
  to=out.getName();
}
