{
  int index=tripTimes.size();
  tripTimes.add(new TripTimes(trip,index,stopTimes));
  trips.add(trip);
  perTripFlags.add((trip.getWheelchairAccessible() == 1 ? TripPattern.FLAG_WHEELCHAIR_ACCESSIBLE : 0) | (((trip.getRoute().getBikesAllowed() == 2 && trip.getTripBikesAllowed() != 1) || trip.getTripBikesAllowed() == 2) ? TripPattern.FLAG_BIKES_ALLOWED : 0));
}
