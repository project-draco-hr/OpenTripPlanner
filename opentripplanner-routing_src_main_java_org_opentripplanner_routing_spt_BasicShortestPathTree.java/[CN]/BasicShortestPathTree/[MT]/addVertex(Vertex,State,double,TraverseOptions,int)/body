{
  SPTVertex ret=this.vertices.get(vv);
  if (ret == null) {
    ret=new SPTVertex(vv,ss,weightSum,options,hops);
    this.vertices.put(vv,ret);
  }
 else {
    if (hops < ret.hops) {
      ret.hops=hops;
    }
    if (weightSum < ret.weightSum) {
      ret.weightSum=weightSum;
      ret.state=ss;
      ret.options=options;
    }
 else {
      return null;
    }
  }
  return ret;
}
