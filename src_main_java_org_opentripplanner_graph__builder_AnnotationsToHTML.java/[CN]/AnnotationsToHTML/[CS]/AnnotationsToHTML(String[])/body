{
  fromStandalone=false;
  jc=new JCommander(this);
  jc.addCommand(annotationEndpoints);
  try {
    jc.parse(args);
  }
 catch (  Exception e) {
    System.out.println(e.getMessage());
    jc.usage();
    System.exit(1);
  }
  if (help || jc.getParsedCommand() == null) {
    jc.usage();
    System.exit(0);
  }
}
