{
  SPTVertex end=null;
  Collection<SPTVertex> set=getVertices(dest);
  if (set == null) {
    return null;
  }
  for (  SPTVertex v : set) {
    if (end == null || v.weightSum < end.weightSum) {
      end=v;
    }
  }
  GraphPath ret=new GraphPath();
  while (true) {
    ret.vertices.add(0,end);
    if (end.incoming == null) {
      break;
    }
    ret.edges.add(0,end.incoming);
    end=end.incoming.fromv;
  }
  if (optimize) {
    ret.optimize();
  }
  return ret;
}
