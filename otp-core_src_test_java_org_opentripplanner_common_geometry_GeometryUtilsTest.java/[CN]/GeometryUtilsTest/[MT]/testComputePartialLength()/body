{
  Coordinate[] coordinates=new Coordinate[4];
  coordinates[0]=new Coordinate(0,0);
  coordinates[1]=new Coordinate(0,3);
  coordinates[2]=new Coordinate(4,3);
  coordinates[3]=new Coordinate(0,0);
  assertEquals(0.0,GeometryUtils.computePartialLength(coordinates,Integer.MIN_VALUE),0.0);
  assertEquals(0.0,GeometryUtils.computePartialLength(coordinates,-1),0.0);
  assertEquals(0.0,GeometryUtils.computePartialLength(coordinates,0),0.0);
  assertEquals(3.0,GeometryUtils.computePartialLength(coordinates,1),0.0);
  assertEquals(7.0,GeometryUtils.computePartialLength(coordinates,2),0.0);
  assertEquals(12.0,GeometryUtils.computePartialLength(coordinates,3),0.0);
  assertEquals(12.0,GeometryUtils.computePartialLength(coordinates,4),0.0);
  assertEquals(12.0,GeometryUtils.computePartialLength(coordinates,5),0.0);
  assertEquals(12.0,GeometryUtils.computePartialLength(coordinates,Integer.MAX_VALUE),0.0);
}
