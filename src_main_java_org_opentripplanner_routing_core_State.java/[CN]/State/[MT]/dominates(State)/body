{
  if (other.weight == 0) {
    return false;
  }
  if (isBikeRenting() != other.isBikeRenting())   return false;
  if (isCarParked() != other.isCarParked())   return false;
  if (backEdge != other.getBackEdge() && ((backEdge instanceof PlainStreetEdge) && (!((PlainStreetEdge)backEdge).getTurnRestrictions().isEmpty())))   return false;
  if (this.routeSequenceSubset(other)) {
    return this.weight <= other.weight && this.getElapsedTimeSeconds() <= other.getElapsedTimeSeconds();
  }
  boolean walkDistanceBetter=walkDistance <= other.getWalkDistance() * 1.05;
  double weightRatio=this.weight / other.weight;
  boolean weightBetter=(weightRatio < 1.02 && this.weight - other.weight < 30);
  boolean timeBetter=this.getElapsedTimeSeconds() - other.getElapsedTimeSeconds() <= 30;
  return walkDistanceBetter && weightBetter && timeBetter;
}
