{
  StringBuilder sb=new StringBuilder();
  int nHops=tt.getNumHops();
  for (int hop=0; hop < nHops; hop++) {
    sb.append('[');
    sb.append(hop);
    sb.append(']');
    sb.append(tt.getDepartureTime(hop));
    sb.append(' ');
    sb.append(tt.getArrivalTime(hop));
  }
  return sb.toString();
}
