{
  List<AlertPatch> alertPatches=child.backEdge.getAlertPatches();
  boolean display=false, active=false;
  if (alertPatches != null) {
    for (    AlertPatch alertPatch : alertPatches) {
      display=false;
      active=alertPatch.activeDuring(child.stateData.opt,child.getStartTimeSeconds(),child.getTimeSeconds());
      if (!active) {
        display=alertPatch.displayDuring(child.stateData.opt,child.getStartTimeSeconds(),child.getTimeSeconds());
      }
      if (display || active) {
        alertPatch.filterTraverseResult(this);
      }
    }
  }
}
