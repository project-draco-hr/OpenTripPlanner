{
  List<Patch> patches=child.backEdge.getPatches();
  boolean display=false, active=false;
  if (patches != null) {
    for (    Patch patch : patches) {
      display=false;
      active=patch.activeDuring(child.stateData.opt,child.getStartTimeSeconds(),child.getTimeSeconds());
      if (!active) {
        display=patch.displayDuring(child.stateData.opt,child.getStartTimeSeconds(),child.getTimeSeconds());
      }
      if (display || active) {
        if (!patch.filterTraverseResult(this,display))         return false;
      }
    }
  }
  return true;
}
