{
  StringBuilder sb=new StringBuilder();
  sb.append(String.format("Edge from %d to %d. Length %f meters, speed %d kph.",getFromVertex(),getToVertex(),getLengthMm() / 1000D,getSpeed()));
  for (  Flag flag : Flag.values()) {
    if (getFlag(flag)) {
      sb.append(" ");
      sb.append(flag.toString());
    }
  }
  return sb.toString();
}
