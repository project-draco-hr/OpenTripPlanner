{
  CoordinateSequence cs=((LineString)(edge.getGeometry())).getCoordinateSequence();
  int s=seg + 1;
  double x0=cs.getX(s);
  double y0=cs.getY(s);
  double dist=distanceLibrary.fastDistance(y0,x0,y,x);
  int nc=cs.size();
  for (; s < nc; s++) {
    double x1=cs.getX(s);
    double y1=cs.getY(s);
    dist+=distanceLibrary.fastDistance(y0,x0,y1,x1);
    x0=x1;
    y0=y1;
  }
  return dist;
}
