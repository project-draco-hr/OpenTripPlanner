{
  String str=null;
  int val=-1;
  try {
    str=reader.get(column);
    val=Integer.parseInt(str);
  }
 catch (  NumberFormatException nfe) {
    String msg=String.format("Error parsing int from string '%s' from column '%s' at line number X. " + "This does not look like an integer.",str,column);
    errorList.add(new Error(msg));
  }
  return val;
}
