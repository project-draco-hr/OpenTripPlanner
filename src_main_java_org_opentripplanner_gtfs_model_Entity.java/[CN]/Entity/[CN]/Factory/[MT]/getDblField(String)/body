{
  String str=null;
  double val=0;
  try {
    str=reader.get(column);
    val=Double.parseDouble(str);
  }
 catch (  NumberFormatException nfe) {
    String msg=String.format("Error parsing double from string '%s' from column '%s' at line number X. " + "This does not look like an double.",str,column);
    errorList.add(new Error(msg));
  }
  return val;
}
