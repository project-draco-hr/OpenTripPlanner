{
  final float f=200;
  loadPixels();
  for (int i=0; i < width * height; i++) {
    int c=pixels[i];
    float r=red(c);
    float g=green(c);
    float b=blue(c);
    float avg=(r + g + b) / 3;
    r+=(avg - r) / f;
    g+=(avg - g) / f;
    b+=(avg - b) / f;
    pixels[i]=color(r,g,b);
  }
  updatePixels();
}
