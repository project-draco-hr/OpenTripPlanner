{
  for (  Stop stop : _dao.getAllStops()) {
    _graph.addVertex(new GenericVertex(id(stop.getId()),stop.getLon(),stop.getLat(),stop.getName(),TransitStop.class));
  }
  if (verbose) {
    System.out.println("Loading hops");
  }
  GTFSPatternHopFactory hf=new GTFSPatternHopFactory(_context);
  hf.run(_graph);
}
