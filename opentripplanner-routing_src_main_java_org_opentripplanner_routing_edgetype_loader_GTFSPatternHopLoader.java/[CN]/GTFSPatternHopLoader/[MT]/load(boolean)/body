{
  for (  Stop stop : _dao.getAllStops()) {
    _graph.addVertex(new TransitStop(id(stop.getId()),stop.getLon(),stop.getLat(),stop.getName(),stop.getId().getId(),stop));
  }
  if (verbose) {
    System.out.println("Loading hops");
  }
  GTFSPatternHopFactory hf=new GTFSPatternHopFactory(_context);
  hf.run(_graph);
}
