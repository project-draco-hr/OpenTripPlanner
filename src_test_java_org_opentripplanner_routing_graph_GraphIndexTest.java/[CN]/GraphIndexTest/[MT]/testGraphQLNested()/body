{
  String query="query Agency{\n" + "    agency(id: \"agency\"){\n" + "        name\n"+ "        routes{\n"+ "            shortName"+ "        }"+ "    }\n"+ "}\n";
  Object result=graph.index.graphQL.execute(query);
  assertEquals(18,((List)((Map)((Map)result).get("agency")).get("routes")).size());
}
