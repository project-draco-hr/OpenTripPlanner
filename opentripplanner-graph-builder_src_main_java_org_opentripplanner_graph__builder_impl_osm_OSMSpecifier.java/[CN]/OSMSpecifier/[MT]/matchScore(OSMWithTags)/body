{
  int score=0;
  for (  P2<String> pair : kvpairs) {
    String tag=pair.getFirst();
    String value=pair.getSecond();
    String matchValue=match.getTag(tag);
    if (value.equals("*") && matchValue != null) {
      score+=1;
    }
 else     if (value.equals(matchValue)) {
      score+=100;
    }
 else {
      return 0;
    }
  }
  return score;
}
