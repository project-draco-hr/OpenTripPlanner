{
  if (value.equals("*") && leftMatchValue != null) {
    return 1;
  }
 else   if (value.equals(leftMatchValue)) {
    return 100;
  }
 else {
    if (value.contains(":")) {
      value=value.split(":",2)[0];
      if (value.equals(leftMatchValue)) {
        return 75;
      }
 else {
        return 0;
      }
    }
 else {
      return 0;
    }
  }
}
