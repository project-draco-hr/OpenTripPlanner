{
  if (rentingBike != other.rentingBike)   return false;
  if (arriveBy) {
    return nBoardings <= other.nBoardings && walkDistance <= other.walkDistance * 1.1 && arrivalTime >= other.arrivalTime;
  }
 else {
    return nBoardings <= other.nBoardings && walkDistance <= other.walkDistance * 1.1 && arrivalTime <= other.arrivalTime;
  }
}
