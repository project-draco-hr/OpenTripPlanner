{
  coverage=gridCoverageFactory.getGridCoverage();
  double total=0, count=0;
  for (  Vertex vv : graph.getVertices()) {
    total+=vv.getY();
    count++;
  }
  averageLatitude=total / count;
  count=0;
  for (  Vertex vv : graph.getVertices()) {
    for (    Edge ee : vv.getOutgoing()) {
      if (ee instanceof Street) {
        processEdge((Street)ee);
      }
    }
  }
}
