{
  if (x < xMin)   xMin=x;
  if (x > xMax)   xMax=x;
  if (y < yMin)   yMin=y;
  if (y > yMax)   yMax=y;
  long x0=((long)x + INDEX_OFFSET) >> shift;
  long y0=((long)y + INDEX_OFFSET) >> shift;
  Long key=x0 + (y0 << 32);
  T[] ts=chunks.get(key);
  if (ts == null) {
    ts=(T[])(new Object[matSize]);
    chunks.put(key,ts);
  }
  int index=((x & mask) << shift) + (y & mask);
  if (ts[index] == null)   size++;
  ts[index]=t;
  return t;
}
