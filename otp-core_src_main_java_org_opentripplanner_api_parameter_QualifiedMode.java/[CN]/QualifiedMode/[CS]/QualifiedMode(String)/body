{
  String[] elements=qMode.split("_");
  mode=TraverseMode.valueOf(elements[0]);
  if (mode == null) {
    throw new InvalidParameterException();
  }
  for (int i=0; i < elements.length; i++) {
    Qualifier q=Qualifier.valueOf(elements[i]);
    if (q == null) {
      throw new InvalidParameterException();
    }
 else {
      qualifiers.add(q);
    }
  }
}
