{
  ShortestPathTree spt=null;
  if (_shortestPathTreeFactory != null)   spt=_shortestPathTreeFactory.create(opts);
  if (spt == null) {
    if (opts.getModes().isTransit()) {
      spt=new MultiShortestPathTree(opts);
    }
 else {
      spt=new BasicShortestPathTree(opts);
    }
  }
  return spt;
}
