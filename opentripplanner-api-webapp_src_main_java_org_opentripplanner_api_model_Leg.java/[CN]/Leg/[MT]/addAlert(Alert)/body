{
  if (notes == null) {
    notes=new ArrayList<Note>();
  }
  if (alerts == null) {
    alerts=new ArrayList<Alert>();
  }
  Note note=new Note(alert.alertHeaderText.getSomeTranslation());
  if (!notes.contains(note)) {
    notes.add(note);
  }
  if (!alerts.contains(alert)) {
    alerts.add(alert);
  }
}
