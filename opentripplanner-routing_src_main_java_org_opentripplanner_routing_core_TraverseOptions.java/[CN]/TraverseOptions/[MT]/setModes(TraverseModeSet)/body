{
  this.modes=modes;
  if (modes.getBicycle()) {
    speed=5;
    boardCost=10 * 60;
    walkingOptions=new TraverseOptions();
    walkingOptions.setArriveBy(this.isArriveBy());
    walkingOptions.maxWalkDistance=maxWalkDistance;
    walkingOptions.walkReluctance*=40.0;
  }
 else   if (modes.getCar()) {
    speed=15;
    walkingOptions=new TraverseOptions();
    walkingOptions.setArriveBy(this.isArriveBy());
    walkingOptions.maxWalkDistance=maxWalkDistance;
  }
}
