{
  String[] parts=attrId.split(":");
  String cat=parts[0];
  String attr=parts[1];
  Category category=getCategoryForId(cat);
  Attribute attribute=category.attributes.get(attr);
  if (attribute == null) {
    attribute=new Attribute(attr);
    category.attributes.put(attr,attribute);
  }
  return attribute;
}
