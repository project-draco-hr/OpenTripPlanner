{
  if (rctx == null) {
    this.rctx=new RoutingContext(this,graph,null,null,true);
    this.rctx.check();
  }
 else {
    if (rctx.graph == graph) {
      LOG.debug("keeping existing routing context");
      return;
    }
 else {
      LOG.error("attempted to reset routing context using a different graph");
      return;
    }
  }
}
