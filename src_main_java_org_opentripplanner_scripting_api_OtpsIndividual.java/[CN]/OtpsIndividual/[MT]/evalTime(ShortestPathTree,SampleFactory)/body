{
  Graph sptGraph=spt.getOptions().getRoutingContext().graph;
  if (!isSampleSet || graph != sptGraph) {
    cachedSample=sampleFactory.getSample(lon,lat);
    graph=sptGraph;
    isSampleSet=true;
  }
  long time=cachedSample.eval(spt);
  if (time == Long.MAX_VALUE)   return null;
  return time;
}
