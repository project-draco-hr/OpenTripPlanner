{
  if (node.isTag("amenity","bicycle_rental")) {
    _bikeRentalNodes.add(node);
    return;
  }
  if (!(_nodesWithNeighbors.contains(node.getId()) || _areaNodes.contains(node.getId()) || node.isStop()))   return;
  if (_nodes.containsKey(node.getId()))   return;
  _nodes.put(node.getId(),node);
  if (_nodes.size() % 100000 == 0)   LOG.debug("nodes=" + _nodes.size());
}
