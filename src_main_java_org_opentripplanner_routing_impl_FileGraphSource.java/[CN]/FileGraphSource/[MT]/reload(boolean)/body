{
synchronized (this) {
    if (preEvict) {
      Graph oldGraph=graph;
      graph=null;
      if (oldGraph != null)       decorator.shutdownGraph(oldGraph);
      graph=loadGraph();
    }
 else {
      Graph newGraph=loadGraph();
      Graph oldGraph=graph;
      graph=newGraph;
      if (oldGraph != null)       decorator.shutdownGraph(oldGraph);
    }
    return (graph != null);
  }
}
