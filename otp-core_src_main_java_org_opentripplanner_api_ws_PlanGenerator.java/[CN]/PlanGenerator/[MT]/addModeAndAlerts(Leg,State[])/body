{
  for (  State state : states) {
    TraverseMode mode=state.getBackMode();
    Set<Alert> alerts=state.getBackAlerts();
    if (mode != null) {
      leg.mode=mode.toString();
    }
    if (alerts != null) {
      for (      Alert alert : alerts) {
        leg.addAlert(alert);
      }
    }
  }
}
