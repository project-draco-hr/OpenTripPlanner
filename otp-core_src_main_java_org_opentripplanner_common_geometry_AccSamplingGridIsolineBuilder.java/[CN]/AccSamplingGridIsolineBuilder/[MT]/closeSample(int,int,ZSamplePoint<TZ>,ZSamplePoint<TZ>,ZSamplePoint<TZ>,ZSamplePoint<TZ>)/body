{
  ZSamplePoint<TZ> A=sampleGrid.getOrCreate(x,y);
  if (A.getUserData() == null)   A.setUserData(new IsoLineUserData());
  A.setZ(zFunc.closeSample(A.up() != null ? A.up().getZ() : null,A.down() != null ? A.down().getZ() : null,A.right() != null ? A.right().getZ() : null,A.left() != null ? A.left().getZ() : null));
  return A;
}
