{
  Class<? extends Object> entityType=entity.getClass();
  if (entity instanceof IdentityBean<?> && _entityReplacementStrategy.hasReplacementEntities(entityType)) {
    IdentityBean<?> bean=(IdentityBean<?>)entity;
    Serializable id=bean.getId();
    if (_entityReplacementStrategy.hasReplacementEntity(entityType,id))     return;
  }
  _store.save(context,entity);
}
