{
  List<Envelope> regions=new ArrayList<Envelope>();
  for (  GraphVertex gv : _graph.getVertices()) {
    Vertex vertex=gv.vertex;
    if (vertex instanceof TransitStop) {
      Envelope env=new Envelope(vertex.getCoordinate());
      env.expandBy(0.02);
      regions.add(env);
    }
  }
  return regions;
}
