{
  final Vertex v=s.getVertex();
  if (v instanceof StreetLocation) {
    return 0;
  }
  if (v instanceof StreetVertex) {
    if (s.isEverBoarded()) {
      if (streetVerticesNearDestination.contains(v)) {
        return 0;
      }
 else {
        return Double.POSITIVE_INFINITY;
      }
    }
 else {
      if (streetVerticesNearOrigin.contains(v)) {
        return 0;
      }
 else {
        return Double.POSITIVE_INFINITY;
      }
    }
  }
 else {
    return 0;
  }
}
