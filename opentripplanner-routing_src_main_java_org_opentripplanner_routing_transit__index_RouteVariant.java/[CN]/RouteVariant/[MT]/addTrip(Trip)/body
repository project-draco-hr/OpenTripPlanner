{
  if (!trips.contains(trip.getId())) {
    trips.add(trip.getId());
    if (direction == null) {
      direction=trip.getDirectionId();
      tripHeadsigns.add(trip.getTripHeadsign());
    }
 else {
      if (!direction.equals(trip.getDirectionId())) {
        direction=MULTIDIRECTION;
      }
    }
  }
}
