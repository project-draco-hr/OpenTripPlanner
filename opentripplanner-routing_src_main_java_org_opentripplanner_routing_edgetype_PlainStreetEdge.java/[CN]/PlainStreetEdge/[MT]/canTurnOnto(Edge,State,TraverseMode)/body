{
  for (  TurnRestriction restriction : turnRestrictions) {
    if (restriction.type == TurnRestrictionType.ONLY_TURN) {
      if (restriction.to != e && restriction.modes.contains(mode) && restriction.active(state.getTime())) {
        return false;
      }
    }
 else {
      if (restriction.to == e && restriction.modes.contains(mode) && restriction.active(state.getTime())) {
        return false;
      }
    }
  }
  return true;
}
