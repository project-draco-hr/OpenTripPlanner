{
  for (  TurnRestriction restriction : turnRestrictions) {
    if (restriction.type == TurnRestrictionType.ONLY_TURN) {
      if (!e.isEquivalentTo(restriction.to) && restriction.modes.contains(mode) && restriction.active(state.getTime())) {
        return false;
      }
    }
 else {
      if (e.isEquivalentTo(restriction.to) && restriction.modes.contains(mode) && restriction.active(state.getTime())) {
        return false;
      }
    }
  }
  return true;
}
