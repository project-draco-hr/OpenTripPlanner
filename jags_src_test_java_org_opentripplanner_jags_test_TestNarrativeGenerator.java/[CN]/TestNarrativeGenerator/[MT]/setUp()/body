{
  try {
    GtfsContext context=GtfsLibrary.readGtfs(new File(TestConstants.CALTRAIN_GTFS));
    graph=new Graph();
    GTFSHopLoader hl=new GTFSHopLoader(graph,context);
    hl.load();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  NetworkLinker nl=new NetworkLinker(graph);
  nl.createLinkage();
}
