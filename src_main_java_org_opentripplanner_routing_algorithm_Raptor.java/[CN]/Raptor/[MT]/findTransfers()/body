{
  for (Iterator<State> stateIt=store.prevIterator(); stateIt.hasNext(); ) {
    State s0=stateIt.next();
    for (    Edge e : s0.getVertex().getOutgoing()) {
      if (e instanceof SimpleTransfer) {
        State s1=e.traverse(s0);
        if (s1 != null)         store.put(s1);
      }
    }
  }
}
