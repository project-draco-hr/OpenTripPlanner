{
  StringBuilder sb=new StringBuilder("VertexQuery(");
  boolean first=true;
  sb.append("location:");
  if (this.location == null) {
    sb.append("null");
  }
 else {
    sb.append(this.location);
  }
  first=false;
  if (isSetAllowed_modes()) {
    if (!first)     sb.append(", ");
    sb.append("allowed_modes:");
    if (this.allowed_modes == null) {
      sb.append("null");
    }
 else {
      sb.append(this.allowed_modes);
    }
    first=false;
  }
  sb.append(")");
  return sb.toString();
}
