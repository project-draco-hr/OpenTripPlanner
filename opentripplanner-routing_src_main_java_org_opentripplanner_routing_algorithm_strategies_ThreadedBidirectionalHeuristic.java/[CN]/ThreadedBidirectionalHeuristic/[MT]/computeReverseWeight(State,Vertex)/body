{
  final Vertex v=s.getVertex();
  if (v instanceof StreetLocation)   return 0;
  int index=v.getIndex();
  if (index < weights.length) {
    double h=weights[index];
    if (v instanceof StreetVertex)     return h;
    if (closed.get(index)) {
      return h;
    }
 else {
      return maxFound;
    }
  }
 else   return 0;
}
