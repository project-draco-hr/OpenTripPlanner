{
  TableTripPattern pattern=getPatternForTrip(tripUpdate.getTripId());
  if (pattern == null) {
    LOG.debug("No pattern found for tripId {}, skipping UpdateBlock.",tripUpdate.getTripId());
    return false;
  }
  boolean applied=buffer.update(pattern,tripUpdate);
  if (applied) {
    getSnapshot();
  }
  return applied;
}
