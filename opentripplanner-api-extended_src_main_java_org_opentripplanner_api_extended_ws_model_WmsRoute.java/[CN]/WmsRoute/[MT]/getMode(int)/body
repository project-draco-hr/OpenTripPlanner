{
switch (type) {
case 0:
    return "TRAM";
case 1:
  return "SUBWAY";
case 2:
return "RAIL";
case 3:
return "BUS";
case 4:
return "FERRY";
case 5:
return "CABLE_CAR";
case 6:
return "GONDOLA";
case 7:
return "FUNICULAR";
default :
throw new IllegalArgumentException("unknown gtfs route type " + type);
}
}
