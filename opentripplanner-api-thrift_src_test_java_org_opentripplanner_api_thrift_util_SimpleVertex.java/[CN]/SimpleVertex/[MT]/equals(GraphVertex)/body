{
  boolean b=(gv.getLabel() == getLabel() && gv.getName() == getName() && gv.getIn_degree() == getDegreeIn() && gv.getOut_degree() == getDegreeOut());
  if (!b)   return false;
  Location loc=gv.getLocation();
  LatLng ll=loc.getLat_lng();
  b=(ll.getLat() == getY() && ll.getLng() == getX());
  return b;
}
