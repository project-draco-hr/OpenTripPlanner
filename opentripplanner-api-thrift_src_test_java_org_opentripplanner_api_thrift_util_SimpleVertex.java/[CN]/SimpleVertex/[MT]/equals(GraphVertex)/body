{
  boolean b=(gv.getId() == getIndex() && gv.getLabel() == getLabel() && gv.getName() == getName() && gv.getIn_degree() == getDegreeIn() && gv.getOut_degree() == getDegreeOut());
  if (!b)   return false;
  LatLng ll=gv.getLat_lng();
  b=(ll.getLat() == getY() && ll.getLng() == getX());
  return b;
}
