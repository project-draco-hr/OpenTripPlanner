{
  loadTrips();
  Table table=this.getTable("stop_times");
  int i=0;
  for (  String[] record : table) {
    i++;
    if (verbose && i % 1000 == 0) {
      System.out.println("stoptime " + i);
    }
    StopTime stoptime=new StopTime(this,table.getHeader(),record);
    trips.get(stoptime.trip_id).addStopTime(stoptime);
  }
}
