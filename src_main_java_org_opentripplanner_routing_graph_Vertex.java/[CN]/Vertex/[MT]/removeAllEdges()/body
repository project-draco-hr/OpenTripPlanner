{
  for (  Edge e : outgoing) {
    Vertex target=e.getToVertex();
    if (target != null) {
      target.removeIncoming(e);
    }
  }
  for (  Edge e : incoming) {
    Vertex source=e.getFromVertex();
    if (source != null) {
      source.removeOutgoing(e);
    }
  }
  incoming=new Edge[0];
  outgoing=new Edge[0];
}
