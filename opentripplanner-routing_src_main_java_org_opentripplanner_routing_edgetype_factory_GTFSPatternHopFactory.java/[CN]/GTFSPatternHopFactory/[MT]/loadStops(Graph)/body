{
  for (  Stop stop : _dao.getAllStops()) {
    TransitStop stopVertex=new TransitStop(id(stop.getId()),stop.getLon(),stop.getLat(),stop.getName(),stop.getId(),stop);
    graph.addVertex(stopVertex);
    if (stop.getLocationType() != 2) {
      TransitStopArrive arrive=new TransitStopArrive(arrivalVertexId(id(stop.getId())),stop.getLon(),stop.getLat(),stop.getId());
      graph.addVertex(arrive);
      TransitStopDepart depart=new TransitStopDepart(departureVertexId(id(stop.getId())),stop.getLon(),stop.getLat(),stop.getId());
      graph.addVertex(depart);
      new PreAlightEdge(arrive,stopVertex);
      new PreBoardEdge(stopVertex,depart);
    }
  }
}
