{
  StringBuilder sb=new StringBuilder();
  sb.append("routes ");
  for (  Segment segment : segments) {
    sb.append(segment.route);
    sb.append(" ");
  }
  sb.append("via ");
  for (  Segment segment : segments.subList(0,segments.size() - 1)) {
    sb.append(segment.to);
    sb.append(" ");
  }
  return sb.toString();
}
