{
  List<UpdateList> ret=new LinkedList<UpdateList>();
  Collections.sort(mixedUpdates);
  UpdateList ul=null;
  for (  Update u : mixedUpdates) {
    if (ul == null || !ul.tripId.equals(u.tripId)) {
      ul=new UpdateList(u.tripId);
      ret.add(ul);
    }
    ul.updates.add(u);
  }
  return ret;
}
