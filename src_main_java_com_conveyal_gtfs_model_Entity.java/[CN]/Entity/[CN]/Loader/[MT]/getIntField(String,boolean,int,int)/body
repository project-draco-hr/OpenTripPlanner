{
  String str=getFieldCheckRequired(column,required);
  int val=INT_MISSING;
  if (str == null) {
    val=0;
  }
 else   try {
    val=Integer.parseInt(str);
    checkRangeInclusive(min,max,val);
  }
 catch (  NumberFormatException nfe) {
    feed.errors.add(new NumberParseError(tableName,row,column));
  }
  return val;
}
