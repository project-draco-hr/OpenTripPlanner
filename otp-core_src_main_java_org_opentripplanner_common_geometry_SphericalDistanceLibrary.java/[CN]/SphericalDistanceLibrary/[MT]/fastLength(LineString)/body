{
  Coordinate[] coordinates=lineString.getCoordinates();
  Coordinate middle=new Coordinate((coordinates[0].x + coordinates[coordinates.length - 1].x) / 2.0,(coordinates[0].y + coordinates[coordinates.length - 1].y) / 2.0);
  return equirectangularProject(middle,lineString).getLength() * RADIUS_OF_EARTH_IN_M;
}
