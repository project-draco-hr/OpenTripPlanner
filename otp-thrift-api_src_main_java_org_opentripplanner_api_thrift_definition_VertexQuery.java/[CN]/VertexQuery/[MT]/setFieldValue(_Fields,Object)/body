{
switch (field) {
case LOCATION:
    if (value == null) {
      unsetLocation();
    }
 else {
      setLocation((org.opentripplanner.api.thrift.definition.Location)value);
    }
  break;
case ALLOWED_MODES:
if (value == null) {
  unsetAllowed_modes();
}
 else {
  setAllowed_modes((Set<org.opentripplanner.api.thrift.definition.TravelMode>)value);
}
break;
}
}
