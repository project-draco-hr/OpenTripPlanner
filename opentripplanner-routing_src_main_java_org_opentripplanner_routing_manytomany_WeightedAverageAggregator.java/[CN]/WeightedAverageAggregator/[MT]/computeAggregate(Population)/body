{
  double result=0;
  int n=0;
  for (  Individual destination : destinations.elements) {
    if (Double.isInfinite(destination.data))     continue;
    if (Double.isInfinite(destination.result))     continue;
    result+=destination.data * destination.result;
    n+=destination.data;
  }
  result/=n;
  return result;
}
