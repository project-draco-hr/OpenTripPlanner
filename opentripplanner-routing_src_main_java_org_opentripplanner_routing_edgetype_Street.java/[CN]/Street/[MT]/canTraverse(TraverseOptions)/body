{
  if (!wheelchairAccessible && wo.wheelchairAccessible)   return false;
  if (wo.modes.getWalk() && permission.allows(StreetTraversalPermission.PEDESTRIAN))   return true;
  if (wo.modes.getBicycle() && permission.allows(StreetTraversalPermission.BICYCLE))   return true;
  if (wo.modes.getCar() && permission.allows(StreetTraversalPermission.CAR))   return true;
  return false;
}
