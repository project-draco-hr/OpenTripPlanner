{
  Vertex stop_g=graph.getVertex("agency_G");
  Vertex stop_h=graph.getVertex("agency_H");
  WalkOptions options=new WalkOptions();
  options.setGtfsContext(context);
  ShortestPathTree spt;
  GraphPath path;
  spt=Dijkstra.getShortestPathTree(graph,stop_g.label,stop_h.label,new State(new GregorianCalendar(2009,8,18,23,20,0).getTimeInMillis()),options);
  path=spt.getPath(stop_h);
  assertNotNull(path);
  assertEquals(4,path.vertices.size());
  spt=Dijkstra.getShortestPathTree(graph,stop_g.label,stop_h.label,new State(new GregorianCalendar(2009,8,19,0,5,0).getTimeInMillis()),options);
  path=spt.getPath(stop_h);
  assertNotNull(path);
  assertEquals(4,path.vertices.size());
}
