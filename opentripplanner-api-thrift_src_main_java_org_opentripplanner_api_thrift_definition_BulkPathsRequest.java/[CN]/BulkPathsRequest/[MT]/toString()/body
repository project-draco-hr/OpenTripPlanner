{
  StringBuilder sb=new StringBuilder("BulkPathsRequest(");
  boolean first=true;
  sb.append("trips:");
  if (this.trips == null) {
    sb.append("null");
  }
 else {
    sb.append(this.trips);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("options:");
  if (this.options == null) {
    sb.append("null");
  }
 else {
    sb.append(this.options);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
