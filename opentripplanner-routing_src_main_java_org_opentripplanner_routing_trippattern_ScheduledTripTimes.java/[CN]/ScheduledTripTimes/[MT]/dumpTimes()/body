{
  StringBuilder sb=new StringBuilder();
  int nHops=getNumHops();
  sb.append(arrivalTimes == null ? "C " : "U ");
  for (int hop=0; hop < nHops; hop++) {
    sb.append(hop);
    sb.append(':');
    sb.append(getDepartureTime(hop));
    sb.append('-');
    sb.append(getArrivalTime(hop));
    sb.append(' ');
  }
  return sb.toString();
}
