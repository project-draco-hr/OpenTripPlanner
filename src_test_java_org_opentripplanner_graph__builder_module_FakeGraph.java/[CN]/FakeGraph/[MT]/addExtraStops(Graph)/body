{
  int count=0;
  double lon=-83;
  for (double lat=40; lat < 40.01; lat+=0.005) {
    String id="EXTRA_" + count++;
    AgencyAndId aid=new AgencyAndId("EXTRA",id);
    Stop stop=new Stop();
    stop.setLat(lat);
    stop.setLon(lon);
    stop.setName(id);
    stop.setCode(id);
    stop.setId(aid);
    new TransitStop(g,stop);
    count++;
  }
  lon=-83.1341 + 0.1;
  for (double lat=39.9058; lat < 40.0281; lat+=0.005) {
    String id="" + count++;
    AgencyAndId aid=new AgencyAndId("EXTRA",id);
    Stop stop=new Stop();
    stop.setLat(lat);
    stop.setLon(lon);
    stop.setName(id);
    stop.setCode(id);
    stop.setId(aid);
    new TransitStop(g,stop);
    count++;
  }
  lon=-83.1341 + 0.15;
  for (double lat=39.9059; lat < 40.0281; lat+=0.005) {
    String id="" + count++;
    AgencyAndId aid=new AgencyAndId("EXTRA",id);
    Stop stop=new Stop();
    stop.setLat(lat);
    stop.setLon(lon);
    stop.setName(id);
    stop.setCode(id);
    stop.setId(aid);
    new TransitStop(g,stop);
    count++;
  }
}
