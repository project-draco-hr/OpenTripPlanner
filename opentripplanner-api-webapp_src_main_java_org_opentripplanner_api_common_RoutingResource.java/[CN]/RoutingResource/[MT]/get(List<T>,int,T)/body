{
  if (l == null || l.size() == 0)   return defaultValue;
  int maxIndex=l.size() - 1;
  if (n > maxIndex)   n=maxIndex;
  T value=l.get(n);
  if (value instanceof Integer) {
    if (value.equals(-1)) {
      return defaultValue;
    }
  }
 else   if (value instanceof Double) {
    if (value.equals(-1.0)) {
      return defaultValue;
    }
  }
  return value;
}
