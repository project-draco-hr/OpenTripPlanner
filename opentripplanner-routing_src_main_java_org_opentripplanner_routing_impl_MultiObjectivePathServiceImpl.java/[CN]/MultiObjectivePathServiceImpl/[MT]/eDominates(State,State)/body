{
  final double EPSILON=0.05;
  if (s0.similarRouteSequence(s1)) {
    return s0.getWeight() <= s1.getWeight() * (1 + EPSILON) && s0.getElapsedTimeSeconds() <= s1.getElapsedTimeSeconds() * (1 + EPSILON) && s0.getWalkDistance() <= s1.getWalkDistance() * (1 + EPSILON) && s0.getNumBoardings() <= s1.getNumBoardings() && (s0.getWeight() < s1.getWeight() || s0.getElapsedTimeSeconds() < s1.getElapsedTimeSeconds() || s0.getWalkDistance() < s1.getWalkDistance() || s0.getNumBoardings() < s1.getNumBoardings());
  }
 else {
    return false;
  }
}
