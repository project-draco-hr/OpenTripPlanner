{
  int bestScore=0;
  float bestSpeed=-1;
  int score;
  for (  SpeedPicker picker : speedPickers) {
    OSMSpecifier specifier=picker.getSpecifier();
    score=specifier.matchScore(way);
    if (score > bestScore) {
      bestScore=score;
      bestSpeed=picker.getSpeed();
    }
  }
  if (bestSpeed != -1)   return bestSpeed;
 else   return this.defaultSpeed;
}
