{
  List<Ride> rides=createRides(path);
  if (rides.size() == 0) {
    return null;
  }
  WrappedCurrency wrappedCurrency=null;
  if (fareAttributes.size() > 0) {
    wrappedCurrency=new WrappedCurrency(fareAttributes.values().iterator().next().getCurrencyType());
  }
  int lowestCost=getLowestCost(rides);
  if (lowestCost < Integer.MAX_VALUE) {
    Fare fare=new Fare();
    fare.addFare(FareType.regular,wrappedCurrency,lowestCost);
    return fare;
  }
 else {
    return null;
  }
}
