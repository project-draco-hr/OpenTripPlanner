{
  byte[] r=new byte[256];
  byte[] g=new byte[256];
  byte[] b=new byte[256];
  byte[] a=new byte[256];
  Arrays.fill(a,(byte)0);
  for (int i=0; i < 30; i++) {
    g[i + 00]=a[i + 00]=b[i + 30]=a[i + 30]=g[i + 60]=r[i + 60]=a[i + 60]=r[i + 90]=a[i + 90]=b[i + 120]=a[i + 120]=r[i + 120]=(byte)(255 - (42 - i) * 6);
  }
  return new IndexColorModel(8,256,r,g,b,a);
}
