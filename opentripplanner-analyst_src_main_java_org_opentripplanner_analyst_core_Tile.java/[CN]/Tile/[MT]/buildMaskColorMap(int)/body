{
  byte[] r=new byte[256];
  byte[] g=new byte[256];
  byte[] b=new byte[256];
  byte[] a=new byte[256];
  for (int i=0; i < max; i++) {
    int alpha=(i * 210 / max);
    a[i]=(byte)alpha;
  }
  for (int i=max; i <= 255; i++) {
    a[i]=(byte)210;
  }
  return new IndexColorModel(8,256,r,g,b,a);
}
