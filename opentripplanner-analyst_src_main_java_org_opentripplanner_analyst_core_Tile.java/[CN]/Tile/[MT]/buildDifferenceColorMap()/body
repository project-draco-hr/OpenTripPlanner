{
  byte[] r=new byte[256];
  byte[] g=new byte[256];
  byte[] b=new byte[256];
  byte[] a=new byte[256];
  Arrays.fill(a,(byte)64);
  for (int i=0; i < 118; i++) {
    g[117 - i]=(byte)(i * 2);
    r[137 + i]=(byte)(i * 2);
    a[117 - i]=(byte)(64 + (i * 2));
    a[137 + i]=(byte)(64 + (i * 2));
  }
  return new IndexColorModel(8,256,r,g,b,a);
}
