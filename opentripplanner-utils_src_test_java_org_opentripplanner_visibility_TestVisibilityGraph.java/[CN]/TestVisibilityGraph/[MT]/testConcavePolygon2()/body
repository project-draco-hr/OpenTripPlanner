{
  Polygon poly=poly(17.0,14.0,70.0,18.0,69.0,26.0,39.0,20.0,13.0,78.0,-111.0,24.0);
  Environment environment=new Environment(Arrays.asList(poly));
  VisibilityGraph vg=new VisibilityGraph(environment,0.0000001);
  boolean expected[][]={{true,true,true,true,true,true},{true,true,true,true,false,true},{true,true,true,true,false,false},{true,true,true,true,true,true},{true,false,false,true,true,true},{true,true,false,true,true,true}};
  for (int i=0; i < expected.length; ++i) {
    for (int j=0; j < expected[i].length; ++j) {
      assertEquals(" at " + i + ", "+ j,expected[i][j],vg.get(i,j));
    }
  }
}
