{
  Preconditions.checkNotNull(stops);
  Preconditions.checkArgument(tripUpdate.getStopTimeUpdateCount() == stops.size(),"number of stop should match the number of stop time updates");
  final String tripId=tripUpdate.getTrip().getTripId();
  cancelScheduledTrip(tripId,serviceDate);
  cancelPreviouslyAddedTrip(tripId,serviceDate);
  final boolean success=addTripToGraphAndBuffer(graph,trip,tripUpdate,stops,serviceDate,RealTimeState.MODIFIED);
  return success;
}
