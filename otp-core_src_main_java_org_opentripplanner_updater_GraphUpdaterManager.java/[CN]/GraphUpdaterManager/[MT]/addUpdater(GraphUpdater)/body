{
  updaterList.add(updater);
  updaterPool.execute(new Runnable(){
    @Override public void run(){
      try {
        updater.setup();
        updater.run();
      }
 catch (      Exception e) {
        LOG.error("Error while running updater " + updater.getClass().getName(),e);
      }
    }
  }
);
}
