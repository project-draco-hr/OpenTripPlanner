{
  for (  Edge e : graph.getIncoming(vertex)) {
    if ((e instanceof TurnEdge || e instanceof OutEdge) && e.getGeometry() != null)     edgeTree.insert(e.getGeometry().getEnvelopeInternal(),e);
  }
  for (  Edge e : graph.getOutgoing(vertex)) {
    if ((e instanceof TurnEdge || e instanceof OutEdge) && e.getGeometry() != null)     edgeTree.insert(e.getGeometry().getEnvelopeInternal(),e);
  }
}
