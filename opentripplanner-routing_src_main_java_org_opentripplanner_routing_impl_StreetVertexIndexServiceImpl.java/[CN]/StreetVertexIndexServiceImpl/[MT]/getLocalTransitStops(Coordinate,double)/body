{
  Envelope env=new Envelope(c);
  env.expandBy(DistanceLibrary.metersToDegrees(distance));
  List<Vertex> nearby=transitStopTree.query(env);
  List<Vertex> results=new ArrayList<Vertex>();
  for (  Vertex v : nearby) {
    if (v.distance(c) <= distance) {
      results.add(v);
    }
  }
  return results;
}
