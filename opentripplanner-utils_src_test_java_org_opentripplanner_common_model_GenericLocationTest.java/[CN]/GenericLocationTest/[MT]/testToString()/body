{
  String input="name::1.0,2.5";
  GenericLocation loc=GenericLocation.fromOldStyleString(input);
  assertEquals(input,loc.toString());
  input="name::12345";
  loc=GenericLocation.fromOldStyleString(input);
  assertEquals(input,loc.toString());
  input="name";
  loc=GenericLocation.fromOldStyleString(input);
  assertEquals(input,loc.toString());
}
