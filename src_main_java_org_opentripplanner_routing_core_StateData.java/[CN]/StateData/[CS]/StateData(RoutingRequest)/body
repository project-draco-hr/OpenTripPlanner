{
  TraverseModeSet modes=options.modes;
  if (modes.getCar())   nonTransitMode=TraverseMode.CAR;
 else   if (modes.getCustomMotorVehicle())   nonTransitMode=TraverseMode.CUSTOM_MOTOR_VEHICLE;
 else   if (modes.getBicycle() && !options.allowBikeRental)   nonTransitMode=TraverseMode.BICYCLE;
 else   if (modes.getWalk())   nonTransitMode=TraverseMode.WALK;
 else   nonTransitMode=null;
}
