{
  LOG.debug("Attempting to load graph '{}' from server's local filesystem.",routerId);
  try {
    otpServer.getGraphService().getGraph(routerId);
    return Response.status(404).entity("graph already registered.\n").build();
  }
 catch (  GraphNotFoundException e) {
    if (preEvict) {
      LOG.debug("Pre-evicting graph '{}'",routerId);
      otpServer.getGraphService().evictGraph(routerId);
    }
    boolean success=otpServer.getGraphService().registerGraph(routerId,otpServer.getGraphService().getGraphSourceFactory().createGraphSource(routerId));
    if (success)     return Response.status(201).entity("graph registered.\n").build();
 else     return Response.status(404).entity("graph not found or other error.\n").build();
  }
}
