{
  StringBuilder sb=new StringBuilder();
  int r=0;
  sb.append("      ");
  for (int i=0; i < table[0].length; i++)   sb.append(String.format("%2d ",i));
  sb.append(" \n");
  for (  int[] row : table) {
    sb.append(String.format("%5s ",states.get(r).label));
    for (    int i : row) {
      if (i == AutomatonState.REJECT)       sb.append(" - ");
 else       sb.append(String.format("%2s ",states.get(i).label));
    }
    sb.append("\n");
    r+=1;
  }
  return sb.toString();
}
