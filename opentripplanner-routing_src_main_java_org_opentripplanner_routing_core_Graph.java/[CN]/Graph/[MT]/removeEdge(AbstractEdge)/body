{
  Vertex fv=e.getFromVertex();
  Vertex fgv=vertices.get(fv.getLabel());
  if (fgv != null) {
    if (fv != fgv)     throw new IllegalStateException("Vertex / edge mismatch.");
 else     fgv.removeOutgoing(e);
  }
  Vertex tv=e.getToVertex();
  Vertex tgv=vertices.get(tv.getLabel());
  if (tgv != null) {
    if (tv != tgv)     throw new IllegalStateException("Vertex / edge mismatch.");
 else     tgv.removeIncoming(e);
  }
}
