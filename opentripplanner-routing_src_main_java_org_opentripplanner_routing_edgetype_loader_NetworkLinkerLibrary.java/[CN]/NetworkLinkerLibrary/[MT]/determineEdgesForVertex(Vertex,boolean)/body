{
  Collection<Vertex> location=getLocation(v);
  if (location == null) {
    getLocation(v);
    return false;
  }
 else {
    for (    Vertex v2 : location) {
      graph.addEdge(new StreetTransitLink(v2,v,wheelchairAccessible));
      graph.addEdge(new StreetTransitLink(v,v2,wheelchairAccessible));
    }
    return true;
  }
}
