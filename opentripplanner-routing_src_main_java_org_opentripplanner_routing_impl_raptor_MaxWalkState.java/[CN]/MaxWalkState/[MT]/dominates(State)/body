{
  if (backEdge != other.getBackEdge() && ((backEdge instanceof PlainStreetEdge) && (!((PlainStreetEdge)backEdge).getTurnRestrictions().isEmpty())))   return false;
  return walkDistance <= other.getWalkDistance() * 1.10 && this.getTime() <= other.getTime() + 30 && getNumBoardings() <= other.getNumBoardings();
}
