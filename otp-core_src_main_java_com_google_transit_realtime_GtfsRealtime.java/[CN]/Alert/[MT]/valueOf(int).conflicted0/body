{
switch (value) {
case 1:
    return UNKNOWN_CAUSE;
case 2:
  return OTHER_CAUSE;
case 3:
return TECHNICAL_PROBLEM;
case 4:
return STRIKE;
case 5:
return DEMONSTRATION;
case 6:
return ACCIDENT;
case 7:
return HOLIDAY;
case 8:
return WEATHER;
case 9:
return MAINTENANCE;
case 10:
return CONSTRUCTION;
case 11:
return POLICE_ACTIVITY;
case 12:
return MEDICAL_EMERGENCY;
default :
return null;
}
}
