{
  super();
  File path=new File(graphName);
  if (path.getName().equals("Graph.obj")) {
    path=path.getParentFile();
  }
  try {
    ObjectInputStream oiStream=new ObjectInputStream(new FileInputStream(new File(graphName)));
    graph=Graph.load(oiStream,Graph.LoadLevel.DEBUG,this.indexFactory);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  pathservice.setGraphService(graphservice);
  pathservice.setSptService(sptService);
  setTitle("VizGui: " + graphName);
  init();
}
