{
  ele=ele.toLowerCase();
  double unit=1;
  if (ele.endsWith("m")) {
    ele=ele.replaceFirst("\\s*m","");
  }
 else   if (ele.endsWith("ft")) {
    ele=ele.replaceFirst("\\s*ft","");
    unit=0.3048;
  }
  try {
    return Double.parseDouble(ele) * unit;
  }
 catch (  NumberFormatException e) {
    return 0.0;
  }
}
