{
  this.networkLinkerLibrary=new NetworkLinkerLibrary(graph,extra);
  this.graph=graph;
  if (graph.hasStreets) {
    if (graph.hasTransit) {
      linkTransit();
    }
    if (graph.hasBikeRental) {
      linkBikeRentalStations();
    }
    if (graph.hasBikeParkRide) {
      linkParkRideStations();
    }
    cleanGraph();
  }
}
