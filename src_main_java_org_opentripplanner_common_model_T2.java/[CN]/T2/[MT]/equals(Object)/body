{
  if (!(object instanceof T2))   return false;
  T2 other=(T2)object;
  if (first == null) {
    if (other.first != null)     return false;
  }
 else {
    if (!first.equals(other.first))     return false;
  }
  if (second == null) {
    if (other.second != null)     return false;
  }
 else {
    if (!second.equals(other.second))     return false;
  }
  return true;
}
