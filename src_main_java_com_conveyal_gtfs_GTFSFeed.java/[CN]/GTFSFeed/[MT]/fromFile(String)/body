{
  GTFSFeed feed=new GTFSFeed();
  ZipFile zip;
  try {
    zip=new ZipFile(file);
    feed.loadFromFile(zip);
    zip.close();
    feed.validate(new DefaultValidator());
    return feed;
  }
 catch (  Exception e) {
    LOG.error("Error loading GTFS: {}",e.getMessage());
    throw new RuntimeException(e);
  }
}
