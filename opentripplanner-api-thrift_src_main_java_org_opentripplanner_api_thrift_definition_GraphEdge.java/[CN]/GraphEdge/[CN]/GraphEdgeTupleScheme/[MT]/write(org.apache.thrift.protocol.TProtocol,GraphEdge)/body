{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  oprot.writeI32(struct.id);
  struct.head.write(oprot);
  struct.tail.write(oprot);
  BitSet optionals=new BitSet();
  if (struct.isSetName()) {
    optionals.set(0);
  }
  oprot.writeBitSet(optionals,1);
  if (struct.isSetName()) {
    oprot.writeString(struct.name);
  }
}
