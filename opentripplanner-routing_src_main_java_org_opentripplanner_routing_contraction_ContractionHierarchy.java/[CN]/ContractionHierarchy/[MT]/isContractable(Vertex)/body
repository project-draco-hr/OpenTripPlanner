{
  if (v instanceof StreetVertex || v instanceof EndpointVertex) {
    for (    Edge e : graph.getOutgoing(v)) {
      Vertex tov=e.getToVertex();
      if (!(tov instanceof StreetVertex || tov instanceof EndpointVertex)) {
        return false;
      }
    }
    return true;
  }
  return false;
}
