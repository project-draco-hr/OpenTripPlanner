{
  List<RouteSpec> ret=new LinkedList<RouteSpec>();
  for (  State s : states) {
    EdgeNarrative e=s.getBackEdgeNarrative();
    Trip trip=e.getTrip();
    if (trip != null) {
      String routeName=GtfsLibrary.getRouteName(trip.getRoute());
      RouteSpec spec=new RouteSpec(trip.getId().getAgencyId(),routeName);
      ret.add(spec);
    }
  }
  return ret;
}
