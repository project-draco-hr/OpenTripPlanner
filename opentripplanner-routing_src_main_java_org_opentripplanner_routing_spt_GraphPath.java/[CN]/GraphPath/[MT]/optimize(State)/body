{
  State ret=s.reversedClone();
  for (State orig=s; orig != null; orig=orig.getBackState()) {
    Edge e=orig.getBackEdge();
    if (e == null)     continue;
    ret=e.traverse(ret);
    copyExistingNarrativeToNewNarrativeAsAppropriate(orig.getBackEdgeNarrative(),ret.getBackEdgeNarrative());
  }
  return ret;
}
