  atp.geometry=(LineString)wr.read("LINESTRING(-82.93727602046642744 39.96934234865877045, -82.98058356730228979 39.96435660398918088, -82.99808255349556418 39.96259692939991481, -83.00091758477827852 39.96357452639394836, -83.00492573245382744 39.98283318717648882, -83.00639212794487776 39.99065396312879273)");
  atp.name="Broad High Express";
  atp.stops=new BitSet();
  for (int i=0; i < 6; i++) {
    if (i == 3)     atp.stops.clear(i);
