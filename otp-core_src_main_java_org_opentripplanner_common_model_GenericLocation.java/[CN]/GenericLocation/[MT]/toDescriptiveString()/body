{
  StringBuilder sb=new StringBuilder();
  sb.append("<GenericLocation lat,lng=").append(this.lat).append(",").append(this.lng);
  if (this.hasHeading()) {
    sb.append(" heading=").append(this.heading);
  }
  if (this.hasEdgeId()) {
    sb.append(" edgeId=").append(this.edgeId);
  }
  sb.append(">");
  return sb.toString();
}
