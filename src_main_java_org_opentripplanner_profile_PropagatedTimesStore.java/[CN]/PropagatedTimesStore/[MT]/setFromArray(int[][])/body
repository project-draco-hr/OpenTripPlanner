{
  for (int i=0; i < times.length; i++) {
    for (int v=0; v < times[i].length; v++) {
      int newValue=times[i][v];
      if (newValue == 0) {
        continue;
      }
      if (mins[v] > newValue) {
        mins[v]=newValue;
      }
      if (maxs[v] < newValue) {
        maxs[v]=newValue;
      }
      sums[v]+=newValue;
      counts[v]+=1;
    }
  }
}
